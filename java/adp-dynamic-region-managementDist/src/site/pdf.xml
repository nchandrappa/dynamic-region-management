<document xmlns="http://maven.apache.org/DOCUMENT/1.0.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DOCUMENT/1.0.1 http://maven.apache.org/xsd/document-1.0.1.xsd"
  outputName="${project.parent.parent.name}">
  
  <!-- Output PDF file name specified by 'outputName' above -->
  <meta>
    <title>${project.parent.parent.name}</title>
  </meta>

  <toc name="Table of Contents">
    <item name="README" ref="../../../../../../README.md"/>
    <item name="Example" ref="test.md"/>
  </toc>

  <cover>
    <coverTitle>Dynamic Region Management</coverTitle>
    <coverSubTitle>v. ${project.version}</coverSubTitle>
    <coverType>Documentation</coverType>
    <projectName>${project.parent.parent.name}</projectName>
  </cover>
  
</document>
